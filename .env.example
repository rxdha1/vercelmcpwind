# Vercel Authentication (used for both Vercel and v0.dev)
VERCEL_TOKEN=

# Vercel Deploy Hook
VERCEL_DEPLOY_HOOK=[https://api.vercel.com/v1/integrations/deploy/<YOUR_DEPLOY_HOOK>](https://api.vercel.com/v1/integrations/deploy/<YOUR_DEPLOY_HOOK>)

# v0 CLI Configuration
COMPONENT_OUTPUT_PATH=
VERCEL_PREVIEW_URL=[https://your-project-name.vercel.app](https://your-project-name.vercel.app)

# Supabase Database Connection. NOTE: DATABASE_URL IS SAME VALUE AS DATABASE_POSTGRES_URL
DATABASE_URL=
DATABASE_POSTGRES_URL=
DATABASE_POSTGRES_PRISMA_URL=
DATABASE_POSTGRES_URL_NON_POOLING=

# Supabase Configuration
DATABASE_SUPABASE_URL=
DATABASE_NEXT_PUBLIC_SUPABASE_URL=
DATABASE_SUPABASE_JWT_SECRET=
DATABASE_SUPABASE_SERVICE_ROLE_KEY= 
DATABASE_NEXT_PUBLIC_SUPABASE_ANON_KEY= 

# Supabase Database Details
DATABASE_POSTGRES_USER=postgres 
DATABASE_POSTGRES_PASSWORD=
DATABASE_POSTGRES_DATABASE=postgres
DATABASE_POSTGRES_HOST=db.randomdigitsorname.supabase.co

# Local SQLite (for development/testing)
SQLITE_PATH=./db/packz.sqlite

# Optional: Bitcoin Address (if your app uses it)
VITE_PLATFORM_BTC_ADDRESS=

# Figma MCP Integration (if needed)
FIGMA_API_KEY=your_figma_api_key_here

# Puppeteer MCP Integration (if needed)
PUPPETEER_API_KEY=your_puppeteer_api_key_here
